{
  "name": "skin-react",
  "version": "2.0.0-beta.43",
  "description": "A set of React Components for Ebay Skin CSS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "npm run storybook",
    "sanity-check": "yarn lint && yarn build && yarn test",
    "script:update-icons": "node scripts/import-icons-types && yarn prettier-lint",
    "build": "npm run clean && tsc && tsc -m es6 --outDir esm --removeComments false && cross-env NODE_ENV=production webpack",
    "build:ci": "yarn lint && yarn coverage && yarn build",
    "version": "npm run build-storybook && npm run build",
    "storybook": "cross-env NODE_ENV=development STORYBOOK=true FAST_REFRESH=false start-storybook -p 6006 -c .storybook",
    "deploy-storybook": "yarn sanity-check && storybook-to-ghpages",
    "prettier-lint": "prettier --write 'src/**/*.{js,jsx,ts,tsx}' '.storybook/**/*.{js,jsx,ts,tsx}'",
    "clean": "shx rm -rf lib esm bundles dist node_modules/.cache .eslintcache",
    "lint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "jest",
    "coverage": "jest --coverage",
    "build-storybook": "build-storybook",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run deploy-storybook"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "classnames": "^2.3.0",
    "core-js": "3",
    "react-focus-lock": "^2.5.0",
    "react-remove-scroll": "^2.4.1",
    "tslib": "^2.1.0",
    "url-loader": "^4.1.1"
  },
  "peerDependencies": {
    "@ebay/skin": "12.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.14",
    "@ebay/skin": "12.3.1",
    "@storybook/addon-a11y": "^6.2.1",
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addon-docs": "^6.2.1",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.2.1",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/addon-viewport": "^6.2.1",
    "@storybook/addons": "^6.2.1",
    "@storybook/core": "^6.2.1",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.1",
    "@storybook/storybook-deployer": "^2.8.7",
    "@testing-library/dom": "^7",
    "@testing-library/jest-dom": "^5",
    "@testing-library/react": "^11",
    "@testing-library/user-event": "^13",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/storybook__react": "^5.2.1",
    "@types/testing-library__jest-dom": "^5",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
